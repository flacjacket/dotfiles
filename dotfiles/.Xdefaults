*background:	#111111
*foreground:	#babdb6
*fadeColor:     #002b36
*cursorColor:   green
*underlineColor:    yellow
*fading:        40

! Black
*color0:    #000000
*color8:    #686773
! Red
*color9:    #fc5166
*color1:  #CC0000
! Green
*color2:  #4E9A06
*color10:   #63ff54
! Yellow
*color3:    #b3751e
*color11:   #ffd75c
! Blue
*color4:   #766eff
*color12: #99b2d1
! Purple
*color5:    #9216a3
*color13:   #e151ff
! Cyan
*color6:    #198a9c
*color14:   #57ffec
! White
