urxvt*cursorColor:		green
urxvt*underlineColor:	yellow
urxvt*scrollBar:		false

urxvt*saveLines:	32767
urxvt*buffered:		true
urxvt*termName:		rxvt
urxvt*loginShell:	true
urxvt*depth:		32
urxvt*scrollTtyOutput:	false
urxvt*scrollTtyKeypress:true
urxvt*secondaryScroll:	true

urxvt*background:	#000000
urxvt*foreground:	#ffffff

! Black
urxvt*color0:  #2E3436
urxvt*color8:  #555753
! Red
urxvt*color1:  #CC0000
urxvt*color9:  #EF2929
! Green
urxvt*color2:  #4E9A06
urxvt*color10: #8AE234
! Yellow
urxvt*color3:  #C4A000
urxvt*color11: #FCE94F
! Blue
urxvt*color4:  #729FCF
urxvt*color12: #99b2d1
! Purple
urxvt*color5:  #75507B
urxvt*color13: #AD7FA8
! Cyan
urxvt*color6:  #06989A
urxvt*color14: #34E2E2
! White
urxvt*color7:  #D3D7CF
urxvt*color15: #EEEEEC

urxvt*font:				xft:Terminus:pixelsize=13
urxvt*boldFont:			xft:Terminus:bold:pixelsize=13
urxvt*italicFont:		xft:Terminus:italic:pixelsize=13
urxvt*boldItalicFont:	xft:Terminus:bold:italic:pixelsize=13

Xft.autohint: 0
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

urxvt.perl-ext-common:		default,matcher
urxvt.matcher.button:		1
urxvt.matcher.pattern.1:	\bwww\.[\w-]\.[\w./?&@#-]*[\w/-]
urxvt*urlLauncher:			chromium

Xcursor.theme:	haematite
